    "author": "Jaivardhan Kapoor",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "20.8.0",
        "@typescript-eslint/eslint-plugin": "6.7.4",
        "@typescript-eslint/parser": "6.7.4",
        "esbuild": "0.19.4",
        "obsidian": "latest",
        "typescript": "5.2.2",
        "tslib": "^2.8.0"
    },
    "dependencies": {
        "tslib": "^2.8.0"
    }
}
